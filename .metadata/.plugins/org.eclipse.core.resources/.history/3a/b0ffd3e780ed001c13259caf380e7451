package codigos;

public class VariavelSoma {
	public static void main(String[] args) { 
	
		int indice = 13;
		int soma = 0;
		int k = 0;
		
		while (k < indice) {
			System.out.println(soma);
			k = k + 1;
			soma += k;
		}
		
		
	
	}
	

	public int resultadoSoma(int soma) {
		
		
		return soma;
		
	}
}
