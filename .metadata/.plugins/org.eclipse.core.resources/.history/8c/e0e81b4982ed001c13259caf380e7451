package codigos;

public class VariavelSoma {
	public static void main(String[] args) {}
	
		private int indice = 13;
		private int soma = 0;
		private int k = 0;
		
		
		
		
		public int getSoma() {
			while (k < indice) {
				k = k + 1;
				soma = soma + k;
				System.out.println(soma);
			}
			
			return soma;
		}
		
}


